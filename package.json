{
  "name": "n8n-nodes-imanage",
  "version": "0.1.0",
  "description": "n8n nodes for iManage Work API integration",
  "keywords": [
    "n8n-community-node-package",
    "imanage",
    "document-management",
    "legal-tech"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Mark Richman"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/IManageApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/IManage/IManage.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
